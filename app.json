{
  "expo": {
    "name": "Smarthealth-ID",
    "slug": "smarthealthid",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "deploymentTarget": "15.1"
    },
    "android": {
      "package": "com.yourcompany.smarthealthid",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "buildToolsVersion": "31.0.0",
            "packagingOptions": {
              "pickFirst": [
                "mipmap-*/ic_launcher.png",
                "mipmap-*/ic_launcher_foreground.png",
                "mipmap-*/ic_launcher.webp",
                "mipmap-*/ic_launcher_foreground.webp"
              ]
            }
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b0866d54-9fd6-417f-a07e-3f082bd96f6a"
      }
    },
    "owner": "takwen"
  }
}
